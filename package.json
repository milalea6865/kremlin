{
  "name": "kremlin",
  "version": "0.0.6",
  "description": "Modern WebApp Pentesting Suite",
  "author": "Zachary Iles",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/zackiles/kremlin.git",
    "type": "git"
  },
  "keywords": [
    "pentest",
    "fuxzzer",
    "exploit",
    "scanner",
    "crawler",
    "attack"
  ],
  "homepage": "https://github.com/zackiles/kremlin",
  "bugs": {
    "url": "https://github.com/zackiles/kremlin/issues"
  },
  "scripts": {
    "release": "standard-version --no-verify"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog-emoji"
    }
  },
  "babel": {
    "presets": [
      "es2015-node6",
      "stage-0"
    ],
    "env": {
      "production": {
        "plugins": [
          "add-module-exports"
        ],
        "presets": [
          "minify",
          "es2015"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:import/errors"
    ],
    "plugins": [
      "promise",
      "import"
    ]
  },
  "snyk": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-minify": "^0.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "codecov": "^2.3.0",
    "commitizen": "^2.9.6",
    "conventional-changelog-angular-emoji": "^0.2.2",
    "conventional-changelog-lint": "^2.1.1",
    "cross-env": "^5.0.5",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^3.5.0",
    "greenkeeper-postpublish": "^1.0.1",
    "markdown-link-check": "^3.1.2",
    "markdownlint-cli": "^0.3.1",
    "mocha": "^3.5.0",
    "mochawesome": "^2.3.0",
    "mochawesome-report-generator": "^2.2.2",
    "ncu": "^0.2.1",
    "nodemon": "^1.12.0",
    "ntl": "^1.2.0",
    "shx": "^0.2.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "snyk": "^1.25.2",
    "standard-changelog": "^1.0.3",
    "standard-version": "^4.2.0",
    "travis-lint": "^1.0.0"
  },
  "dependencies": {
    "yargs": "^8.0.2"
  },
  "engines": {
    "npm": "^5.0.0",
    "node": "^7.0.0"
  }
}
